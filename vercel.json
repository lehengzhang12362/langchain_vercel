{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd nextjs-service && npm install && npm run build",
  "outputDirectory": "nextjs-service/.next",
  "installCommand": "cd nextjs-service && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/nextjs-service/$1"
    }
  ]
}

